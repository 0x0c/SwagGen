{
    "formatter": "Swift",
    "files":
    [
        {
            "template": "Podspec.stencil",
            "path": "{{ options.name }}.podspec"
        },
        {
            "template": "API.stencil",
            "path": "API.swift"
        },
        {
            "template": "Networking.stencil",
            "path": "Networking.swift"
        },
        {
            "template": "model.stencil",
            "context": "definitions",
            "path": "Models/{{ name }}.swift"
        },
        {
            "template": "request.stencil",
            "context": "operations",
            "path": "Requests/{{ tag|upperCamelCase }}/{{ operationId|upperCamelCase }}.swift"
        }
    ],
    "options":
    {
        "name": "API",
    }
}
